{
  "name": "varnish-agent",
  "instances": 1,
  "merge_logs": true,
  "max_memory_restart": "100M",
  "script": "app.js",
  "log_file": "/var/log/varnish-agent.log",
  "error_file": "/var/log/varnish-agent-error.log",
  "out_file": "/var/log/varnish-agent-out.log",
  "args" : ["-c", "./default.json"],
  "env": {
    "NODE_ENV": "production"
  }
}
